{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src"
    ],
    "entryPointStrategy": "expand",
    "out": "docs/api",
    "exclude": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/__tests__/**",
        "**/node_modules/**"
    ],
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": true,
    "readme": "none",
    "plugin": [],
    "theme": "default",
    "hideGenerator": true,
    "sort": [
        "source-order"
    ],
    "navigation": {
        "includeCategories": true,
        "includeGroups": true
    }
}